<%
var id = arg['incdir']+arg['dir']['img']+'/header';
-%>
    <!-- Header Start -->
    <header class="l-header" data-js="header">
        <div class="p-header" id="header">
            <div class="p-header__inner">
                <div class="p-header__leftwrap">
                    <a href="#">
                        <picture class="logo">
                            <img src="<%- id %>/logo@2x.png" width="94" height="94" alt="medical salon μ">
                        </picture>
                    </a>
                    <h1 class="p-header__ttl">
                        <picture>
                            <source media="(min-width: 768px)" type="image/png" srcset="<%- id %>/ttl.png, <%- id %>/ttl@2x.png 2x">
                            <img src="<%- id %>/ttl.png"
                            width="252" height="87" alt="medical salon μ −miuex 札幌中央 −">
                        </picture>
                    </h1>
            </div>

                <div class="p-header-btn__wrapper u-display__tab-pc">
                    <a href="#" class="c-btn">
                        <picture>
                            <source media="(min-width: 768px)" type="image/png" srcset="<%- id %>/line.png, <%- id %>/line@2x.png 2x">
                            <img src="<%- id %>/line@2x.png" width="112" height="30" alt="LINE問い合わせ">
                        </picture>
                    </a>
                </div>
            </div>
        </div>
    </header>
    <!-- Header End -->
